{
  "name": "twitch-stream-team",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "dependencies": {
    "@apollo/client": "^3.6.5",
    "axios": "^1.3.4",
    "camelcase": "^7.0.1",
    "graphql": "^16.5.0",
    "image-validator": "^1.2.0",
    "prop-types": "^15.8.1",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.0",
    "react-is": "^18.2.0",
    "react-textfit": "^1.1.0",
    "react-use": "^17.3.2",
    "react-uwp": "1.3.4",
    "styled-components": "^5.3.8",
    "url-search-params-polyfill": "^8.1.1",
    "whatwg-fetch": "3.6.2"
  },
  "scripts": {
    "start": "PORT=8080 vite",
    "build": "vite build",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "vitest",
    "eslint": "eslint --color --ext .html,.js,.json,.jsx,.md,.ts,.tsx ./src",
    "eslint:fix": "npm run eslint -- --fix",
    "vite:start": "vite"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^3.0.1",
    "bfj": "^7.0.2",
    "eslint": "^8.14.0",
    "eslint-config-adjunct": "^4.11.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-babel": "^0.2.1",
    "eslint-config-auto": "^0.7.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "vite": "^4.0.4",
    "vite-plugin-svgr": "^2.4.0",
    "vite-react-jsx": "^1.1.2",
    "vitest": "^0.29.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
